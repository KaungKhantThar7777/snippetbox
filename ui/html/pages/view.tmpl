{{define "title"}}Snippet #{{.Snippet.ID}}{{end}}


{{define "main"}}
    <div class="snippet">
        <div class="metadata">
            <strong>{{.Snippet.Title}}</strong>
            <span>#{{.Snippet.ID}}</span>
        </div>
        <pre>{{.Snippet.Content}}</pre>

        <div class='metadata'>
            <time>Created: {{.Snippet.Created}}</time>
            <time>Expires: {{.Snippet.Expires}}</time> </div>
        </div>
{{end}}